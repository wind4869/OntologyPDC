  <owl:DatatypeProperty rdf:ID="totalSalary">
    <rdfs:domain rdf:resource="#SalaryRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="title">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Media"/>
          <owl:Class rdf:about="#Order"/>
          <owl:Class rdf:about="#Goods"/>
          <owl:Class rdf:about="#Activity"/>
          <owl:Class rdf:about="#Review"/>
          <owl:Class rdf:about="#Writing"/>
          <owl:Class rdf:about="#Course"/>
          <owl:Class rdf:about="#Project"/>
        </owl:unionOf>
      </owl:Class>
