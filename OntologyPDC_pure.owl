    <rdfs:domain rdf:resource="#SalaryRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ee">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
