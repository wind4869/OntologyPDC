    <rdfs:domain rdf:resource="#Order"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="basicSalary">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
