      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#User"/>
          <owl:Class rdf:about="#SalaryRecord"/>
          <owl:Class rdf:about="#Project"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
