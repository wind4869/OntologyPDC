          <owl:Class rdf:about="#Account"/>
          <owl:Class rdf:about="#AccumulatePoint"/>
          <owl:Class rdf:about="#Activity"/>
          <owl:Class rdf:about="#Collection"/>
          <owl:Class rdf:about="#Course"/>
          <owl:Class rdf:about="#Entity"/>
          <owl:Class rdf:about="#Location"/>
          <owl:Class rdf:about="#MobilePhone"/>
          <owl:Class rdf:about="#Order"/>
          <owl:Class rdf:about="#Record"/>
          <owl:Class rdf:about="#Review"/>
          <owl:Class rdf:about="#SalaryRecord"/>
          <owl:Class rdf:about="#UserCredit"/>
          <owl:Class rdf:about="#Writing"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
